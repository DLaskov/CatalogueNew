@model ProductListViewModel

@{
    ViewBag.Title = "Wishlist";
}

<h2 class="headline4">Wishlist</h2>
@if (Model != null)
{
    @Html.Partial("_RenderProductsPartial", Model)

    @section Scripts{
        <script src="~/scripts/wishlist.js"></script>
    }
}
else
{
    @Html.Partial("_NotFound")
    return;
}
